    <Json>{"Code":"using System;\r\nusing System.IO;\r\n\r\nusing Clifton.Core.Semantics;\r\nusing Clifton.Core.ServiceInterfaces;\r\nusing Clifton.Core.ServiceManagement;\r\n\r\nnamespace App\r\n{\r\n\tpublic partial class AppReceptor : IReceptor\r\n\t{\r\n        // Change ISemanticType to the specific semantic message to process...\r\n\t\tpublic void Process(ISemanticProcessor proc, IMembrane membrane, PlayOrStop msg)\r\n\t\t{\r\n\t\t\tConsole.WriteLine(\"PlayOrStop\");\r\n\r\n\t\t\tif (!Program.Playing)\r\n            {\r\n                MemoryStream stream = Program.CreateWavStream(Program.Player, Program.Frequency);\r\n                Program.Player.Stream = stream;\r\n                Program.Player.PlayLooping();\r\n                stream.Close();\r\n                // btnPlay.Text = \"Stop\";\r\n                Program.Playing = true;\r\n            }\r\n            else\r\n            {\r\n                Program.Player.Stop();\r\n                // btnPlay.Text = \"Play\";\r\n                Program.Playing = false;\r\n            }\r\n\t\t}\r\n\t}\r\n}\r\n"}</Json>
