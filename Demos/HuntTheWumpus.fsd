    <Json>{"Code":"using Clifton.Core.Semantics;\r\nusing Clifton.Core.ServiceInterfaces;\r\nusing Clifton.Core.ServiceManagement;\r\n\r\nnamespace HuntTheWumpus\r\n{\r\n\tpublic partial class UIReceptor : IReceptor\r\n\t{\r\n        // Change ISemanticType to the specific semantic message to process...\r\n\t\tpublic void Process(ISemanticProcessor proc, IMembrane membrane, ST_ButtonClick msg)\r\n\t\t{\r\n\t\t\tSystem.Console.WriteLine(msg.ShapeName);\r\n\t\t\t\r\n\t\t\tswitch(msg.ShapeName)\r\n\t\t\t{\r\n\t\t\t\tcase \"btnMove1\":\r\n\t\t\t\t\tproc.ProcessInstance&lt;CaveMembrane, ST_Move&gt;(m=&gt;m.CaveIndex = 0);\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"btnMove2\":\r\n\t\t\t\t\tproc.ProcessInstance&lt;CaveMembrane, ST_Move&gt;(m=&gt;m.CaveIndex = 1);\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"btnMove3\":\r\n\t\t\t\t\tproc.ProcessInstance&lt;CaveMembrane, ST_Move&gt;(m=&gt;m.CaveIndex = 2);\r\n\t\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t}\r\n    }\r\n}\r\n"}</Json>
