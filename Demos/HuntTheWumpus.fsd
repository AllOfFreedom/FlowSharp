    <Json>{"Code":"using System.Drawing;\r\n\r\nusing Clifton.Core.Semantics;\r\nusing Clifton.Core.ServiceInterfaces;\r\nusing Clifton.Core.ServiceManagement;\r\n\r\nusing App;\r\n\r\nnamespace HuntTheWumpus\r\n{\r\n\tpublic partial class CaveReceptor : IReceptor\r\n\t{\r\n\t\t// This is a synchronous message, so an interlocked increment of counters is not necessary.\r\n\t\tpublic void Process(ISemanticProcessor proc, IMembrane membrane, ST_DescribeCave msg)\r\n\t\t{\r\n\t\t\tif (HasSuperBats) \r\n\t\t\t{\r\n\t\t\t\t++CaveReceptor.batIndicatorCounter; \r\n\t\t\t\tHelpers.SetBackgroundColor(\"indBats\" + batIndicatorCounter, Color.Red);\r\n\t\t\t\tSystem.Console.WriteLine(\"I hear flapping!\");\r\n\t\t\t}\r\n\t\t\t\r\n\t\t\tif (HasBottomlessPit)\r\n\t\t\t{\r\n\t\t\t\t++CaveReceptor.pitIndicatorCounter; \r\n\t\t\t\tHelpers.SetBackgroundColor(\"indPit\" + pitIndicatorCounter, Color.Red);\r\n\t\t\t\tSystem.Console.WriteLine(\"I feel a draft!\");\r\n\t\t\t}\r\n\t\t\t\r\n\t\t\tif (HasWumpus) \r\n\t\t\t{\r\n\t\t\t\t++CaveReceptor.wumpusIndicatorCounter; \r\n\t\t\t\tHelpers.SetBackgroundColor(\"indWumpus\", Color.Red);\r\n\t\t\t\tSystem.Console.WriteLine(\"I smell a Wumpus!\");\r\n\t\t\t}\r\n\t\t}\r\n    }\r\n}\r\n"}</Json>
