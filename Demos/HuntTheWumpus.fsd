    <Json>{"Code":"using System;\r\nusing System.Drawing;\r\n\r\nusing Clifton.Core.Semantics;\r\nusing Clifton.Core.ServiceInterfaces;\r\nusing Clifton.Core.ServiceManagement;\r\n\r\nusing App;\r\n\r\nnamespace HuntTheWumpus\r\n{\r\n\tpublic partial class CaveReceptor : IReceptor\r\n\t{\r\n\t\tpublic void Process(ISemanticProcessor proc, IMembrane membrane, ST_AdjacentCaveStatus msg)\r\n\t\t{\r\n\t\t\tpitIndicatorCounter = 0;\r\n\t\t\tbatIndicatorCounter = 0;\r\n\t\t\twumpusIndicatorCounter = 0;\r\n\t\t\tConsole.WriteLine(\"You are in cave # \" + CaveNumber);\r\n\t\t\tHelpers.SetText(\"caveNumber\", \"You are in cave \" + CaveNumber);\r\n\t\t\t\r\n\t\t\tadjacentCaves.ForEachWithIndex((ac, n) =&gt; \r\n\t\t\t{\r\n\t\t\t\tproc.ProcessInstance&lt;CaveMembrane, ST_DescribeCave&gt;(c =&gt; c.CaveNumber = ac, true);\r\n\t\t\t\tHelpers.SetText(\"btnMove\" + (n+1), \"Move To \" + ac);\r\n\t\t\t});\r\n\t\t\t\r\n\t\t\t// Clear unused indicators.  We do this last so we don't have to deal with async HttpGet receive issues.\r\n\t\t\t\r\n\t\t\tConsole.WriteLine(\"Pits = \" + CaveReceptor.pitIndicatorCounter);\r\n\t\t\tfor (int i=CaveReceptor.pitIndicatorCounter+1; i&lt;4; i++)\r\n\t\t\t{\r\n\t\t\t\tHelpers.SetBackgroundColor(\"indPit\" + i, Color.LightGray);\r\n\t\t\t}\r\n\t\t\t\r\n\t\t\tConsole.WriteLine(\"Bats = \" + CaveReceptor.batIndicatorCounter);\r\n\t\t\tfor (int i=CaveReceptor.batIndicatorCounter+1; i&lt;4; i++)\r\n\t\t\t{\r\n\t\t\t\tHelpers.SetBackgroundColor(\"indBats\" + i, Color.LightGray);\r\n\t\t\t}\r\n\t\t\t\r\n\t\t\tConsole.WriteLine(\"Wumpus = \" + CaveReceptor.wumpusIndicatorCounter);\r\n\t\t\tif (CaveReceptor.wumpusIndicatorCounter == 0)\r\n\t\t\t{\r\n\t\t\t\tHelpers.SetBackgroundColor(\"indWumpus\", Color.LightGray);\r\n\t\t\t}\r\n\t\t}\r\n    }\r\n}\r\n"}</Json>
